<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Regression Test Suite" verbose="1" >
    <!-- To run in parallel you can put this command -> parallel="tests" thread-count="2" after verbose -->
    <listeners>
        <listener class-name="util.AllureReportListener"/>
    </listeners>
    <test name="Signin Tests">
        <classes>
            <class name="signin.SignInTest" />
            <class name="signin.SignUpTest" />
            <class name="signin.ForgotPasswordTest" />
        </classes>
    </test>


</suite>